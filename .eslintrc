{
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "quotes": ["error", "single"],
    "semi": "error",
    "indent": ["error", 2],
    "no-trailing-spaces": "error",
    "comma-dangle": "error",
    "eol-last": "error"
  }
}
