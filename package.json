{
  "name": "nodesera",
  "version": "0.3.2",
  "description": "Paysera Library for Node.js",
  "repository": "Kasparas-G55/nodesera",
  "author": "Kasparas Galdikas",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/index.cjs",
    "./dist/index.mjs",
    "./dist/index.d.ts",
    "./dist/public.key"
  ],
  "scripts": {
    "build": "unbuild && cp ./src/public.key ./dist",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "test": "pnpm lint && vitest run --coverage"
  },
  "keywords": [
    "paysera",
    "api",
    "payment processing"
  ],
  "bugs": {
    "url": "https://github.com/Kasparas-G55/nodesera/issues"
  },
  "homepage": "https://github.com/Kasparas-G55/nodesera#readme",
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.7.1"
}
