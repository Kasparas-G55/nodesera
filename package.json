{
  "name": "nodesera",
  "version": "0.0.0",
  "description": "Paysera Library for Node.js",
  "repository": "Kasparas-G55/nodesera",
  "author": "Kasparas Galdikas",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "test": "pnpm lint && vitest run --coverage"
  },
  "keywords": ["paysera", "api", "payment processing"],
  "bugs": {
    "url": "https://github.com/Kasparas-G55/nodesera/issues"
  },
  "homepage": "https://github.com/Kasparas-G55/nodesera#readme",
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.3.1"
  },
  "packageManager": "pnpm@8.15.4"
}
